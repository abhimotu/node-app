    "executionRoleArn": "arn:aws:iam::615817570482:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "nodeapp",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
